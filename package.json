{
  "name": "cac",
  "version": "1.0.0",
  "description": "Cac",
  "main": "./build/cac.js",
  "scripts": {
    "dev": "ts-node-dev --env-file=env/.development.env src/cac.ts",
    "build": "rm -rf ./build && tsc",
    "lint": "ts-standard --fix",
    "start": "node --env-file=env/.production.env build/cac.js",
    "test": "jest --verbose",
    "testbd": "npx jest --testPathPattern='bd.test.ts' --verbose"
  },
  "keywords": [
    "cac"
  ],
  "author": "Andres Arango",
  "license": "ISC",
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "./node_modules/ts-standard/eslintrc.json",
    "env": {
      "jest": true
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/test/**/*.test.ts"
    ]
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.19.2",
    "mariadb": "^3.3.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.7",
    "mysql2-promise": "^0.1.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-jest-mocker": "^1.0.0",
    "ts-node-dev": "^2.0.0",
    "ts-standard": "^12.0.2",
    "typescript": "^5.4.5"
  }
}
